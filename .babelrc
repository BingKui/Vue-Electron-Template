{
    "presets": [
        ["env", {
            "modules": "commonjs",
            "targets": {
                "browsers": ["> 1%", "last 2 versions", "not ie <= 8"]
            }
        }],
        "stage-0"
    ],
    "plugins": [
        "add-module-exports",
        "transform-runtime",
        "transform-decorators-legacy",
        ["import", {
            "libraryName": "iview",
            "libraryDirectory": "src/components"
        }]
    ]
}